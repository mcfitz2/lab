---
apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: overseer
  namespace: media
spec:
  ingressClassName: cloudflare-tunnel
  rules:
    - http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: overseer
                port:
                  number: 5055
      host: "{{ overseer_fqdn }}"