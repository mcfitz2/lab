---
apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: oauth
  namespace: greatlakes
spec:
  ingressClassName: cloudflare-tunnel
  rules:
    - http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: oauth
                port:
                  number: 4002
      host: "{{ oauth_service_fqdn }}"