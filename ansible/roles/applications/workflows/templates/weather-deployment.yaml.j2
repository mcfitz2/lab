apiVersion: apps/v1
kind: Deployment
metadata:
  annotations:
    kompose.cmd: ../../ansible/kompose convert
    kompose.version: 1.34.0 (cbf2835db)
  labels:
    io.kompose.service: weather
  name: weather
spec:
  replicas: 1
  selector:
    matchLabels:
      io.kompose.service: weather
  template:
    metadata:
      annotations:
        kompose.cmd: ../../ansible/kompose convert
        kompose.version: 1.34.0 (cbf2835db)
      labels:
        io.kompose.service: weather
    spec:
      imagePullSecrets: 
        - name: gitlab-credentials
      containers:
        - env:
            - name: PREFECT_API_KEY
              value: "{{ lookup('bitwarden.secrets.lookup', 'a967dcc7-6410-4a51-a9e8-b202013ce6e4') }}"
            - name: PREFECT_API_URL
              value: "{{ lookup('bitwarden.secrets.lookup', 'fa0d0056-4ed9-46d4-b211-b202013d0a83') }}"
          image: registry.gitlab.com/mcfitz2/lab/weather:latest
          name: weather
      restartPolicy: Always
