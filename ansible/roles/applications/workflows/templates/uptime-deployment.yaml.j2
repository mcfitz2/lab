apiVersion: apps/v1
kind: Deployment
metadata:
  annotations:
    kompose.cmd: ../../ansible/kompose convert
    kompose.version: 1.34.0 (cbf2835db)
  labels:
    io.kompose.service: uptime
  name: uptime
spec:
  replicas: 1
  selector:
    matchLabels:
      io.kompose.service: uptime
  template:
    metadata:
      annotations:
        kompose.cmd: ../../ansible/kompose convert
        kompose.version: 1.34.0 (cbf2835db)
      labels:
        io.kompose.service: uptime
    spec:
      imagePullSecrets: 
        - name: gitlab-credentials
      containers:
        - env:
            - name: PREFECT_ACCOUNT_ID
              value: 6661ec6b-f68e-42a9-bec1-fcc2b961e3c4
            - name: PREFECT_API_KEY
              value: "{{ lookup('bitwarden.secrets.lookup', 'a967dcc7-6410-4a51-a9e8-b202013ce6e4') }}"
            - name: PREFECT_WORKSPACE_ID
              value: 7e45c4b7-3704-45cc-88da-ee84c415c970
          image: registry.gitlab.com/mcfitz2/lab/uptime:latest
          name: uptime
          ports:
            - containerPort: 4003
              protocol: TCP
      restartPolicy: Always
